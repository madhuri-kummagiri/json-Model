<mvc:View controllerName="app.firstjsonmodel.controller.DetailForm"

    xmlns:mvc="sap.ui.core.mvc"
     xmlns:f="sap.ui.layout.form"
    xmlns="sap.m">
    
    <Page title="{i18n>title1}"  showNavButton="true" navButtonPress="onPressToView1">
     <f:SimpleForm  editable="true">
        <f:content>
            <Label text="EMP ID"></Label>
            <Text text="{madhuModel>/EMPID}" />
            
             <Label text="EMP NAME"></Label>
            <Text text="{madhuModel>/EMPNAME}"></Text>
             <Label text="EMP COMPANY"></Label>
            <Text text="{madhuModel>/COMPANY}"></Text>
             <Label text="EMP GENDER"></Label>
            <Text text="{madhuModel>/EMPGENDER}"></Text>
             <Label text="EMP STATE"></Label>
            <Text text="{madhuModel>/EMPSTATE}"></Text>

        </f:content>

    </f:SimpleForm>
   
  
    

    
    
</Page>
</mvc:View>